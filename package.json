{
  "name": "maturity-360",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "NODE_OPTIONS=--max-old-space-size=4096 next build",
    "build:prod": "NODE_OPTIONS=--max-old-space-size=4096 next build",
    "start": "next start",
    "deploy:db": "tsx scripts/check-env.ts && drizzle-kit push && tsx scripts/seed-production.ts",
    "lint": "eslint",
    "db:push": "tsx scripts/check-env.ts && drizzle-kit push",
    "db:push:force": "tsx scripts/check-env.ts && drizzle-kit push --force",
    "db:migrate": "tsx scripts/check-env.ts && drizzle-kit generate && drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx scripts/check-env.ts && npx tsx scripts/seed-database.ts",
    "db:seed:dev": "tsx scripts/check-env.ts && npx tsx scripts/seed-dev.ts",
    "db:seed:prod": "tsx scripts/check-env.ts && npx tsx scripts/seed-production.ts",
    "db:reset:dev": "tsx scripts/check-env.ts && npx tsx scripts/seed-dev.ts",
    "db:enable-rls": "tsx scripts/check-env.ts && psql $DATABASE_URL -f scripts/enable-rls.sql",
    "postinstall": "cp node_modules/pdfjs-dist/build/pdf.worker.min.mjs public/pdf.worker.min.mjs"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.11.1",
    "@aws-sdk/client-s3": "^3.958.0",
    "@aws-sdk/s3-request-presigner": "^3.958.0",
    "@neondatabase/serverless": "^1.0.2",
    "@phosphor-icons/react": "^2.1.10",
    "@types/bcryptjs": "^2.4.6",
    "@types/nodemailer": "^7.0.4",
    "@types/pg": "^8.16.0",
    "bcryptjs": "^3.0.3",
    "bufferutil": "^4.1.0",
    "dompurify": "^3.3.1",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "googleapis": "^169.0.0",
    "html2canvas": "^1.4.1",
    "next": "^16.1.1",
    "next-auth": "^5.0.0-beta.30",
    "nodemailer": "^7.0.11",
    "pdfjs-dist": "^4.8.69",
    "pg": "^8.16.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hot-toast": "^2.6.0",
    "react-pdf": "^10.2.0",
    "stripe": "^20.1.0",
    "tsx": "^4.21.0",
    "utf-8-validate": "^6.0.6",
    "ws": "^8.18.3",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-pdf": "^6.2.0",
    "@types/ws": "^8.18.1",
    "eslint": "^9",
    "eslint-config-next": "15.1.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vercel": "^50.1.3"
  }
}